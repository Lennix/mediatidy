{
  "private": true,
  "preferGlobal": true,
  "main": "index",
  "name": "mediatidy",
  "description": "Keep your media nice and Tidy!",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:tkdan235/mediatidy-cli.git"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "directories": {
    "bin": "bin",
    "lib": "lib"
  },
  "dependencies": {
    "async": "^0.9.0",
    "colors": "^0.6.2",
    "commander": "^2.1.0",
    "crypto": "0.0.3",
    "fs-extra": "^0.11.1",
    "lodash": "^2.4.1",
    "nconf": "^0.6.9",
    "node-dir": "^0.1.6",
    "node-ffprobe": "^1.2.2",
    "node-sha1": "0.0.1",
    "path": "^0.4.9",
    "prompt": "^0.2.14",
    "sqlite3": "^2.2.7"
  },
  "coffeelintConfig": {
    "max_line_length": {
      "name": "max_line_length",
      "value": 120,
      "level": "error",
      "limitComments": true
    }
  }
}
