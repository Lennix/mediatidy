{
  "private": true,
  "preferGlobal": true,
  "main": "index",
  "name": "mediatidy",
  "description": "Keep your media nice and tidy!",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:tkdan235/mediatidy.git"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "directories": {
    "bin": "bin",
    "lib": "lib"
  },
  "dependencies": {
    "async": "^0.9.0",
    "coffee-script": "^1.8.0",
    "colors": "^0.6.2",
    "commander": "~2.1.0",
    "fs-extra": "^0.11.1",
    "node-dir": "^0.1.6",
    "node-ffprobe": "^1.2.2",
    "prompt": "^0.2.14",
    "sqlite3": "^2.2.7"
  },
  "coffeelintConfig": {
    "max_line_length": {
      "name": "max_line_length",
      "value": 120,
      "level": "error",
      "limitComments": true
    }
  }
